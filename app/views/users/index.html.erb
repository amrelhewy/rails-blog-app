<div class="container c1">

<%= image_tag 'logo' %>
 <% if flash[:errors] %>
<% flash[:errors].each do |e| %>
<p class="text-danger"><%= e %></p>
 <% end %>
  <% end %>
<%= form_tag sessions_path do %>
<div class="form-group">
 <%= label_tag :email %>
 <%= email_field_tag :email ,nil,{:class=>'form-control'} %>

</div>
<div class="form-group">
 <%= label_tag :password  %>
 <%= password_field_tag :password ,nil,{:class=>'form-control'} %>
 
</div>
<%= submit_tag "Sign In",:class=>'form-control btn btn-primary' %>
<%= link_to "Register", { :controller => :users, :action => :new }, { class: "form-control btn btn-primary mt-2" } %>

<% end %>
</div>